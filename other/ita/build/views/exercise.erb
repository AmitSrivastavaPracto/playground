<h1>Exercise <%= @exercise.name %></h1>
<%= yield %>
<% if @exercise.graph? %>
  <hr />
  <img src="<%= '%02d/%02d/%02d.png' % @exercise.components %>" />
<% end %>
<% @exercise.languages.each do |language| %>
  <hr />
  <h3><%= language.to_s.capitalize %> code</h3>
  <%= Renderer.render_code @exercise.code_path(language), language %>
<% end %>
